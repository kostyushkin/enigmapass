{
    "name": "EnigmaPass",
    "description": "Generate secure, per-domain password based on an original master password, with the click of a button",
    "version": "1.2",
    "background_page": "background.html",
    "options_page": "options.html",
    "permissions":  
        [
	        "tabs", "http://*/*", "https://*/*"
	    ],
    "browser_action":
        {
            "default_title": "EnigmaPass",
            "default_icon": "icon_19.png",
            "popup": "popup.html"
        },
    "content_scripts":
        [{
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery.js","enigmapass.js","supergenpass.js"],
		}],
    "icons":
        {
            "128": "icon_128.png",
            "19": "icon_19.png",
            "16": "icon_16.png"
        }
}
